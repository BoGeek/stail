{
  "name": "@stail/plugins",
  "version": "0.5.0",
  "author": "Alex Grand <alex@granted.name>",
  "license": "MIT",
  "description": "Use TailwindCSS styled components",
  "main": "dist/cjs/src/index.js",
  "module": "dist/mjs/src/index.js",
  "types": "dist/mjs/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mjs/src/index.d.ts",
      "import": "./dist/mjs/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BoGeek/stail.git"
  },
  "peerDependencies": {
    "tailwindcss": "*"
  },
  "scripts": {
    "build": "yarn run -T tsc -p tsconfig.json && yarn run -T tsc -p tsconfig.mjs.json && ../../fixup.sh"
  }
}
